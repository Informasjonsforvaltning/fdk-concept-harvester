                </admin>
                <transacGrp>
                    <transac type="ansvarligVirksomhet">985942897</transac>
                </transacGrp>
                <langSec xml:lang="en">
                    <descrip type="definisjon">item being operated upon by a power tool</descrip>
                    <termSec>
                        <term>workpiece</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
                <langSec xml:lang="nb">
                    <descrip type="definisjon">gjenstand som blir bearbeidet av et kraftdrevet verktøy</descrip>
                    <termSec>
                        <term>arbeidsstykke</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
            </conceptEntry>
