            <conceptEntry id="44f32700-9603-4ebc-9b97-ff4eab5cfa47">
                <admin type="identifikator">https://sd-tl-dev-search-app.azurewebsites.net/term/163589240400769/deu
                </admin>
                <transacGrp>
                    <transac type="ansvarligVirksomhet">985942897</transac>
                </transacGrp>
                <langSec xml:lang="en">
                    <descrip type="definisjon">operations necessary to restore a reusable packaging to a functional
                        state
                    </descrip>
                    <termSec>
                        <term>reconditioning</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
                <langSec xml:lang="nb">
                    <descrip type="definisjon">operasjoner som er nødvendige for å føre gjenbruksemballasjen tilbake til
                        funksjonsdyktig stand
                    </descrip>
                    <termSec>
                        <term>renovering</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
            </conceptEntry>
            <conceptEntry id="65764219-74e7-4ce9-a184-3fed78334d9f">
                <admin type="identifikator">https://sd-tl-dev-search-app.azurewebsites.net/term/163589240500798/deu
                </admin>
                <transacGrp>
                    <transac type="ansvarligVirksomhet">985942897</transac>
                </transacGrp>
                <langSec xml:lang="en">
                    <descrip type="definisjon">packaging is empty if - under normal and foreseeable circumstances - all
                        product residues that can be removed by the emptier have been removed using practices commonly
                        employed for that type of packaging
                        A non-exhaustive list of common practices includes: removing an inner liner; pouring; pumping;
                        aspirating; shaking; scraping; squeezing; rinsing; wiping-out.
                    </descrip>
                    <termSec>
                        <term>empty packaging</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
                <langSec xml:lang="nb">
                    <descrip type="definisjon">en emballasje er tom når brukeren, under normale og forventede forhold,
                        har tømt den for alle produktrester som det er mulig å fjerne, ved hjelp av metoder som er
                        vanlig for emballasjetypen. En ikke-uttømmende liste over slike vanlige metoder er gitt
                        nedenfor: fjerne et innerbelegg; helle ut; pumpe ut; suge ut; riste ut; skrape ut; presse ut;
                        spyle ut; tørke ut.
                    </descrip>
                    <termSec>
                        <term>tom emballasje</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
            </conceptEntry>
            <conceptEntry id="70b71008-abf9-4aad-9494-15095eaa21ae">
                <admin type="identifikator">https://sd-tl-dev-search-app.azurewebsites.net/term/163589243903265/deu
                </admin>
                <transacGrp>
                    <transac type="ansvarligVirksomhet">985942897</transac>
                </transacGrp>
                <langSec xml:lang="en">
                    <descrip type="definisjon">plane rigid surface upon which the tile is fixed</descrip>
                    <termSec>
                        <term>fixing surface</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
                <langSec xml:lang="nb">
                    <descrip type="definisjon">plan, stiv overflate som flisen legges på</descrip>
                    <termSec>
                        <term>underlag</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
            </conceptEntry>
            <conceptEntry id="90799b9b-d9f7-4c31-8113-be5a46b5342a">
                <admin type="identifikator">https://sd-tl-dev-search-app.azurewebsites.net/term/163589244103424/deu
                </admin>
                <transacGrp>
                    <transac type="ansvarligVirksomhet">985942897</transac>
                </transacGrp>
                <langSec xml:lang="en">
                    <descrip type="definisjon">maximum time interval during which the tile’s position in the adhesive
                        layer is able to be adjusted without significant loss of adhesion strength
                    </descrip>
                    <termSec>
                        <term>adjustability</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
                <langSec xml:lang="nb">
                    <descrip type="definisjon">lengste tidsrom hvor det er mulig å justere flisens stilling i limlaget
                        uten vesentlig tap av adhesjonsfasthet
                    </descrip>
                    <termSec>
                        <term>justerbarhet</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
            </conceptEntry>
            <conceptEntry id="9b19b61e-8204-411e-aca5-465c0315b7e3">
                <admin type="identifikator">https://sd-tl-dev-search-app.azurewebsites.net/term/163589244203511/deu
                </admin>
                <transacGrp>
                    <transac type="ansvarligVirksomhet">985942897</transac>
                </transacGrp>
                <langSec xml:lang="en">
                    <descrip type="definisjon">structure for separating sand and other mineral matter from wastewater,
                        using air to induce circulation. It may be combined with the removal of floating matter
                    </descrip>
                    <termSec>
                        <term>aerated grit chamber</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
                <langSec xml:lang="nb">
                    <descrip type="definisjon">konstruksjon for å separere sand og annet mineralsk materiale fra
                        avløpsvann ved bruk av luft for å indusere sirkulasjon. Den kan kombineres med fjerning av
                        flytende materiale
                    </descrip>
                    <termSec>
                        <term>luftet sandfang</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
            </conceptEntry>
            <conceptEntry id="a1a621fd-adb3-4807-8b10-452306bb749e">
                <admin type="identifikator">https://sd-tl-dev-search-app.azurewebsites.net/term/163589244203524/deu
                </admin>
                <transacGrp>
                    <transac type="ansvarligVirksomhet">985942897</transac>
                </transacGrp>
                <langSec xml:lang="en">
                    <descrip type="definisjon">wastewater lagoon with artificial aeration and without sludge
                        recirculation
                    </descrip>
                    <termSec>
                        <term>aerated lagoon</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
                <langSec xml:lang="nb">
                    <descrip type="definisjon">avløpsdam med kunstig lufting og uten resirkulering av slam</descrip>
                    <termSec>
                        <term>luftet biologisk dam</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
            </conceptEntry>
            <conceptEntry id="e56c0bb5-1f98-4df6-99ff-ef4fbedded4d">
                <admin type="identifikator">https://sd-tl-dev-search-app.azurewebsites.net/term/163589244203537/deu
                </admin>
                <transacGrp>
                    <transac type="ansvarligVirksomhet">985942897</transac>
                </transacGrp>
                <langSec xml:lang="en">
                    <descrip type="definisjon">introduction of air or oxygen</descrip>
                    <termSec>
                        <term>aeration</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
                <langSec xml:lang="nb">
                    <descrip type="definisjon">tilførsel av luft eller oksygen</descrip>
                    <termSec>
                        <term>lufting</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
            </conceptEntry>
            <conceptEntry id="29ec33a0-c0b4-4337-83ea-b8863d1d8fc5">
                <admin type="identifikator">https://sd-tl-dev-search-app.azurewebsites.net/term/163589244903975/deu
                </admin>
                <transacGrp>
                    <transac type="ansvarligVirksomhet">985942897</transac>
                </transacGrp>
                <langSec xml:lang="en">
                    <descrip type="definisjon">interchangeable or replaceable attachment which fits into or onto a power
                        tool or machine
                    </descrip>
                    <termSec>
                        <term>inserted tool</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
                <langSec xml:lang="nb">
                    <descrip type="definisjon">utskiftbart eller erstattbart tilbehør som passer i eller på et
                        kraftdrevet verktøy eller en maskin
                    </descrip>
                    <termSec>
                        <term>tilleggsutstyr</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
            </conceptEntry>
            <conceptEntry id="c1a47a19-33d4-430c-8dac-fb8813f95f45">
                <admin type="identifikator">https://sd-tl-dev-search-app.azurewebsites.net/term/163589244903986/deu
                </admin>
                <transacGrp>
                    <transac type="ansvarligVirksomhet">985942897</transac>
                </transacGrp>
                <langSec xml:lang="en">
                    <descrip type="definisjon">identifiable task for which a representative vibration magnitude
                        measurement is made, this may be for the use of a single power tool, or hand-held workpiece type
                        or for a single phase of a task
                    </descrip>
                    <termSec>
                        <term>operation</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
                <langSec xml:lang="nb">
                    <descrip type="definisjon">identifiserbar oppgave der det foretas en representativ måling av
                        størrelsesordenen av vibrasjoner. Denne målingen kan gjelde bruken av et enkelt kraftdrevet
                        verktøy eller av et bestemt håndholdt arbeidsstykke, eller det kan være en fase i
                        arbeidsoppgaven
                    </descrip>
                    <termSec>
                        <term>arbeidsoperasjon</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
            </conceptEntry>
            <conceptEntry id="aac21c9a-de2a-41cc-a6e3-0dfa47d5dbbc">
                <admin type="identifikator">https://sd-tl-dev-search-app.azurewebsites.net/term/163589244903997/deu
                </admin>
                <transacGrp>
                    <transac type="ansvarligVirksomhet">985942897</transac>
                </transacGrp>
                <langSec xml:lang="en">
                    <descrip type="definisjon">person using a hand-fed, hand-guided or hand-held machine or power tool
                    </descrip>
                    <termSec>
                        <term>operator</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
                <langSec xml:lang="nb">
                    <descrip type="definisjon">person som bruker en maskin eller et verktøy som er håndmatet, håndstyrt
                        eller håndholdt
                    </descrip>
                    <termSec>
                        <term>operatør</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
            </conceptEntry>
            <conceptEntry id="c8e8f735-4b36-44ad-8d12-3ce6c22d1a69">
                <admin type="identifikator">https://sd-tl-dev-search-app.azurewebsites.net/term/163589244904008/deu
                </admin>
                <transacGrp>
                    <transac type="ansvarligVirksomhet">985942897</transac>
                </transacGrp>
                <langSec xml:lang="en">
                    <descrip type="definisjon">any period during which a power tool is operating and the operator is
                        being exposed to hand-transmitted vibration
                    </descrip>
                    <termSec>
                        <term>tool operation</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
                <langSec xml:lang="nb">
                    <descrip type="definisjon">enhver periode der et kraftdrevet verktøy er i drift og operatøren
                        eksponeres for håndoverførte vibrasjoner
                    </descrip>
                    <termSec>
                        <term>verktøydrift</term>
                        <termNote type="typeTerm">anbefaltTerm</termNote>
                    </termSec>
                </langSec>
            </conceptEntry>
            <conceptEntry id="70750ca4-e3a6-4c24-8ea6-614eefd5bb08">
